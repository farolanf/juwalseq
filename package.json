{
  "name": "juwal",
  "version": "1.0.0",
  "description": "Juwal",
  "main": "server/index.js",
  "scripts": {
    "start": "nodemon -w server server",
    "start:prod": "LOAD_ENV=1 NODE_ENV=production nodemon -w server server",
    "start:docker": "docker run --rm -p 3000:3000 --dns 172.17.0.1 --env-file .env.docker juwal",
    "dev:prod": "LOAD_ENV=1 NODE_ENV=production node server",
    "container": "nodemon -w server server & cd frontend && npm start",
    "lint": "eslint server/**",
    "reset:dev": "node cmd data:initdb; sequelize db:migrate; sequelize db:seed:all; node cmd data:create-admin admin admin@foo.com",
    "reset:prod": "LOAD_ENV=1 NODE_ENV=production node cmd data:reset",
    "sync:dev": "node -e \"require('./server/sequelize').sequelize.sync({ alter: true }).then(() => process.exit())\"",
    "test": "cd server && NODE_ENV=test mocha --exit --file tests/init.js \"tests/**/*Test.js\"",
    "test:watch": "cd server && NODE_ENV=test mocha -w --exit --file tests/init.js \"tests/**/*Test.js\"",
    "build:templates": "webpack --config server/templates/lib/webpack.config.js --progress --colors && node server/templates/lib/src/build.js",
    "build:templates:watch": "webpack --config server/templates/lib/webpack.config.js --progress --colors -w",
    "build": "docker build -t juwal .",
    "deploy": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farolanf/turing-fullstack-farolan.git"
  },
  "author": "Farolan Faisal <farolanfaisal@gmail.com>",
  "private": true,
  "bugs": {
    "url": "https://github.com/farolanf/turing-fullstack-farolan/issues"
  },
  "homepage": "https://github.com/farolanf/turing-fullstack-farolan#readme",
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@paypal/checkout-server-sdk": "^1.0.0",
    "async": "^2.6.2",
    "babel-loader": "^8.0.5",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "change-case": "^3.1.0",
    "commander": "^2.19.0",
    "continuation-local-storage": "^3.2.1",
    "cors": "^2.8.5",
    "cssnano": "^4.1.10",
    "dotenv": "^6.2.0",
    "elasticsearch": "^15.4.1",
    "express": "^4.16.4",
    "finale-rest": "^1.0.2",
    "generate-password": "^1.4.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "modclean": "^3.0.0-beta.1",
    "module-alias": "^2.2.0",
    "mustache": "^3.0.1",
    "mysql2": "^1.6.5",
    "node-linux": "^0.1.12",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "pluralize": "^7.0.0",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "promise-events": "^0.1.4",
    "prompts": "^2.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-typography": "^0.16.19",
    "sequelize": "^4.42.1",
    "sequelize-cli": "^5.4.0",
    "short-uuid": "^3.1.0",
    "sqlite3": "^4.0.6",
    "tailwindcss": "^0.7.4",
    "typography": "^0.16.19",
    "typography-theme-funston": "^0.16.19",
    "uncss": "^0.16.2",
    "uploadfs": "^1.13.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2",
    "yup": "^0.26.10"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "eslint": "^5.13.0",
    "eslint-plugin-react": "^7.12.4",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10"
  },
  "_moduleAliases": {
    "@root": ".",
    "@server": "server",
    "@db": "server/sequelize",
    "@lib": "server/lib",
    "@config": "server/config",
    "@test": "server/tests"
  }
}
